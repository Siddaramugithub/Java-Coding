public class DecimalToBinary {
	public static void main(String args[]) {
		int decnum = 10,quot, i = 1;
		int binnum[] = new int[100];
		quot = decnum;
		while (quot != 0) {
			binnum[i++] = quot % 2;
			quot = quot / 2;
		}
		for (int j = i-1; j > 0; j--) {
			System.out.print(binnum[j]);
		}
	}
}
